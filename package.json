{
  "name": "tarko-github-agent",
  "version": "0.0.0",
  "description": "A Github Agent",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "tarko-agent",
    "agent-infra",
    "agent",
    "typescript",
    "library"
  ],
  "scripts": {
    "bootstrap": "pnpm i --prefer-offline",
    "example": "tsx examples/index.ts",
    "clean": "rm -rf cjs esm",
    "dev": "rslib build --watch",
    "build": "rslib build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "quick-publish"
  },
  "dependencies": {
    "@agent-infra/browser-search": "^0.0.5",
    "@mcp-agent/core": "0.3.0-beta.1"
  },
  "devDependencies": {
    "tsx": "^4.20.3",
    "@rslib/core": "0.11.0",
    "@types/node": "22.15.17",
    "@vitest/coverage-v8": "3.1.3",
    "prettier": "3.5.3",
    "quick-publish": "0.7.1",
    "typescript": "5.8.3",
    "vitest": "3.1.3"
  },
  "files": [
    "dist"
  ]
}